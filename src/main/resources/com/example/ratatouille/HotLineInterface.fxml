            <DropShadow />
         </effect></TextArea>
      <Button layoutX="54.0" layoutY="335.0" mnemonicParsing="false" onAction="#ComplainsClicked" text="Complains">
         <font>
            <Font name="Times New Roman Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button layoutX="271.0" layoutY="335.0" mnemonicParsing="false" onAction="#InquiresClicked" text="Inquires">
         <font>
            <Font name="Times New Roman Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button layoutX="154.0" layoutY="335.0" mnemonicParsing="false" onAction="#NotesClicked" text="Specific Notes">
         <font>
            <Font name="Times New Roman Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button layoutX="678.0" layoutY="515.0" mnemonicParsing="false" onAction="#OnMenuClicked" prefHeight="50.0" prefWidth="98.0" text="Menu">
