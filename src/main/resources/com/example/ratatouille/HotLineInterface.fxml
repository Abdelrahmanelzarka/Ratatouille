<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ratatouille.HotLineController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="1.0" layoutY="2.0">
         <image>
            <Image url="@../../../Images/IMG_8456.JPG" />
         </image>
      </ImageView>
      <TextArea fx:id="UserMessage" layoutX="17.0" layoutY="372.0" prefHeight="194.0" prefWidth="370.0">
